{
  "manifest_version": 3,
  "name": "AjoccToys",
  "version": "1.3.5",
  "description": "AJOCC公式リザルトページの経過時間をネットラップタイムに変換します。今後要望に応じて機能を拡張します",
  "permissions": ["storage"],
  "host_permissions": ["https://data.cyclocross.jp/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://data.cyclocross.jp/race/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "AjoccToys - クリックでON/OFF切り替え"
  }
}
